<h2> Search Authors </h2>

<form method="GET">
    <label>Name: </label>
    <input type="text" name="name">
    <button type="submit" value="Search" value="<%= searchOptions.name %>">Search</button>
</form>
<br>

<% authors.forEach( author=> { %>
    <div>
        <%=author.name %>
    </div>
    <% }) %>